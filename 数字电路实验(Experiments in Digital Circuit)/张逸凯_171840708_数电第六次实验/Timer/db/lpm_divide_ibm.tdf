FUNCTION sign_div_unsign_olh (denominator[3..0], numerator[31..0])
RETURNS ( quotient[31..0], remainder[3..0]);

--synthesis_resources = lut 182 
SUBDESIGN lpm_divide_ibm
( 
	denom[3..0]	:	input;
	numer[31..0]	:	input;
	quotient[31..0]	:	output;
	remain[3..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_olh;
	numer_tmp[31..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
